{
  "name": "treeple",
  "version": "0.1.0",
  "description": "Conservation grid platform for interactive exploration and symbolic land claiming",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules",
    "reinstall": "npm run clean && npm install",
    "generate-grid": "node scripts/generate-grid.js",
    "seed-tiles": "node scripts/seed-tiles.js",
    "setup": "npm run generate-grid && npm run seed-tiles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/[username]/treeple.git"
  },
  "homepage": "https://github.com/[username]/treeple",
  "bugs": "https://github.com/[username]/treeple/issues",
  "keywords": [
    "conservation",
    "mapping",
    "geospatial",
    "nextjs",
    "maplibre",
    "yellowstone"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@turf/turf": "^7.1.0",
    "class-variance-authority": "^0.7.0",
