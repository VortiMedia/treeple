    "lucide-react": "^0.462.0",
    "maplibre-gl": "^4.7.1",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "pmtiles": "^4.3.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.63.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
