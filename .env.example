# Treeple Environment Variables
# Copy this file to .env.local and fill in your actual values

# MapTiler API Key (required)
# Get a free API key from https://cloud.maptiler.com/
# Sign up for free tier (100k map loads/month)
NEXT_PUBLIC_MAPTILER_KEY=your_api_key_here

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Tile pricing (in cents)
NEXT_PUBLIC_TILE_PRICE=5000

# Phase 2: Database (Supabase)
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=
# SUPABASE_SERVICE_ROLE_KEY=

# Phase 2: Payments (Stripe)
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
# STRIPE_SECRET_KEY=
# STRIPE_WEBHOOK_SECRET=
